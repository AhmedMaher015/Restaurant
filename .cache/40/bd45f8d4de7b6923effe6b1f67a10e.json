  }\n  100% {\n    transform: rotate(360deg);\n  }\n}\n\n/* start cart Items */\n.cart-items {\n  list-style: none;\n  margin: 0;\n  padding: 0;\n  max-height: 30rem;\n  overflow-x: hidden;\n  overflow-y: auto;\n}\n\n.cart-item {\n  display: block;\n  /* justify-content: space-between;\n  align-items: center; */\n  border-bottom: 2px solid #8a2b06;\n  padding: 1rem 0;\n  margin: 1rem 0;\n}\n/* Class to hide  */\n/* .hidden {\n  display: none !important;\n  opacity: 0 !important;\n} */\n\n.cart-item h2 {\n  margin: 0 0 0.5rem 0;\n  color: #363636;\n}\n\n.summary {\n  width: 13rem;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.price {\n  font-weight: bold;\n  color: #8a2b06;\n}\n\n.amount {\n  font-weight: bold;\n  border: 1px solid #ccc;\n  padding: 0.25rem 0.75rem;\n  border-radius: 6px;\n  color: #363636;\n}\n\n.pic {\n  width: 100%;\n  height: 100px;\n  border-radius: 50%;\n}\n/* end cart Items */\n\n/* start form */\n.form__container {\n  background-color: rgba(0, 0, 0, 0.6);\n  display: none;\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n}\n\n.form__container.show__modal {\n  display: block;\n}\n\n.form__modal {\n  background-color: #fff;\n  border-radius: 5px;\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.3);\n  position: absolute;\n  overflow: hidden;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  max-width: 100%;\n  width: 400px;\n  animation: modalopen var(--modal-duration);\n}\n\n.modal__form__header {\n  background-color: var(--primary-color);\n  color: #fff;\n  padding: 15px;\n}\n\n.modal__form__header h3 {\n  margin: 0;\n  border-bottom: 1px solid #333;\n}\n\n.modal__form__content {\n  padding: 20px;\n}\n\n.modal__form div {\n  margin: 15px 0;\n}\n\n.modal__form label {\n  display: block;\n  margin-bottom: 5px;\n}\n\n.modal__form .form-input {\n  padding: 8px;\n  width: 100%;\n}\n\n.close-btn {\n  background: transparent;\n  font-size: 25px;\n  position: absolute;\n  top: 0;\n  right: 0;\n}\n\n.modal__form__content input[type='submit'] {\n  background-color: var(--primary-color);\n  border: 0;\n  border-radius: 5px;\n  color: #fff;\n  cursor: pointer;\n  padding: 8px 12px;\n}\n\nbutton,\ninput[type='submit'] {\n  background-color: var(--secondary-color);\n  border: 0;\n  border-radius: 5px;\n  color: #fff;\n  cursor: pointer;\n  padding: 8px 12px;\n}\nbutton:focus {\n  outline: none;\n}\n\n@keyframes modalopen {\n  form {\n    opacity: 0;\n  }\n  to {\n    opacity: 1;\n  }\n}\n\n/* end form */\n\n/* show recipe modal */\n\n#detailsModal.show {\n  display: block;\n}\n\n/* recpies */\n.recipe {\n  background-color: #fff;\n  border-top: 1px solid #fff;\n}\n.recipe__fig {\n  height: 20rem;\n  position: relative;\n  transform: scale(1.04) translateY(-1px);\n  transform-origin: top;\n  padding-left: 0.6rem;\n  padding-right: 0.6rem;\n}\n\n.recipe__img {\n  width: 100%;\n  display: block;\n  height: 100%;\n  object-fit: cover;\n}\n\n.recipe__title {\n  position: absolute;\n  bottom: 0;\n  left: 50%;\n  transform: translate(-50%, 20%) skewY(-6deg);\n  color: #fff;\n  font-weight: 700;\n  font-size: 1.4rem;\n  text-transform: uppercase;\n  width: 70%;\n  line-height: 2.5;\n  text-align: center;\n}\n\n.recipe__title span {\n  -webkit-box-decoration-break: clone;\n  box-decoration-break: clone;\n  padding: 1rem;\n  background-image: linear-gradient(to right bottom, #fbdb89, #f48982);\n}\n\n.recipe__details {\n  display: flex;\n  align-items: center;\n  justify-content: space-around;\n}\n\n.recipe__info {\n  font-size: 1.5rem;\n  text-transform: uppercase;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding: 1.5rem;\n}\n.recipe__info-icon {\n  height: 2rem;\n  width: 2rem;\n  fill: #f59a83;\n}\n.recipe__info-people {\n  margin-right: 0.4rem;\n  font-weight: 600;\n}\n.recipe__love {\n  background-image: linear-gradient(to right bottom, #fbdb89, #f48982);\n  border: none;\n  cursor: pointer;\n  margin-left: auto;\n  transition: all 0.2s;\n}\n.recipe__love:hover {\n  transform: scale(1.07);\n}\n.recipe__love:focus {\n  outline: none;\n}\n\n.recipe__ingredients {\n  padding: 2rem 1rem;\n  font-size: 1.5rem;\n  line-height: 1.4;\n  background-color: #f2efee;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n.recipe__ingredient-list {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  grid-column-gap: 1.5rem;\n  grid-row-gap: 1.5rem;\n  list-style: none;\n  margin-bottom: 2rem;\n}\n.recipe__item {\n  display: flex;\n  font-size: small;\n}\n.recipe__icon {\n  height: 1.8rem;\n  width: 1.8rem;\n  color: #f59a83;\n  padding: 2px;\n  margin-right: 1rem;\n  flex: 0 0 auto;\n  margin-top: 0.1rem;\n}\n.recipe__count {\n  margin-right: 0.5rem;\n  flex: 0 0 auto;\n}\n\n.btn-small {\n  background-image: linear-gradient(to right bottom, #fbdb89, #f48982);\n  border-radius: 10rem;\n  border: none;\n  /* text-transform: uppercase; */\n  color: #fff;\n  cursor: pointer;\n  display: flex;\n  align-items: center;\n  transition: all 0.2s;\n}\n.btn-small:hover {\n  transform: scale(1.05);\n}\n.btn-small:focus {\n  outline: none;\n}\n.btn-small > *:first-child {\n  margin-right: 1rem;\n}\n/*End recipe details modal*/\n"},"lineCount":330}},"error":null,"hash":"c2e2d1a28799b26a7a73743f618f8196","cacheData":{"env":{}}}